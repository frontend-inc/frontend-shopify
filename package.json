{
    "name": "@webstudio/shopify",
    "version": "1.0.1",
    "license": "MIT",
    "description": "An Apollo GraphQL client for the Shopify Storefront API.",
    "homepage": "https://github.com/webstudio-co/webstudio-shopify",
    "repository": {
        "type": "git",
        "url": "https://github.com/webstudio-co/webstudio-shopify"
    },
    "keywords": [
        "react",
        "shopify",
        "apollo"        
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "babel": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "build_old": "NODE_ENV=production && npx rimraf dist && npx mkdirp dist && npx babel ./src --out-dir dist --copy-files"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
      "cookies-next": "^2.1.1",
      "@apollo/client": "^3.8.9",
      "apollo-link-context": "^1.0.20",
      "apollo-link-http": "^1.5.17"  
    },
    "devDependencies": {
        "cookies-next": "^2.1.1",
        "react": "^17.0.1 || ^18.0.0",
        "react-dom": "^17.0.2 || ^18.0.0",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "gh-pages": "^2.2.0",
        "react-scripts": "^4.0.3",
        "typescript": "^4.4.4"
    },
    "peerDependencies": {
        "@apollo/client": "^3.8.9",
        "apollo-link-context": "^1.0.20",
        "apollo-link-http": "^1.5.17",
        "react": "^17.0.1 || ^18.0.0",
        "react-dom": "^17.0.2 || ^18.0.0"
    }
}
